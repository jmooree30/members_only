<div class='form-title'>Login Form </div>

<div class="form-group">
  <div class="col-md-6 col-md-offset-3">
 <%= form_for(:session, url: login_path ) do |f| %>
<%= f.label :email %>
<%= f.email_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.submit "Login" %>
    <% end %>
  </div>
</div>